{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "node": true,
        "esnext": true,
        "globals": {},
        "globalstrict": true,
        "quotmark": "single",
        "undef": true,
        "unused": true
    }
}